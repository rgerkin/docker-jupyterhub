# Branch name
branch=$(git rev-parse --abbrev-ref HEAD)
if [ $branch == "dev" ]; then
  port=9000
else
  port=8000
fi
echo $branch
echo $port

docker run -p ${port}:8000 -d --name jupyterhub_${branch} -v jupyterhub-etc:/etc -v jupyterhub-home:/home jupyterhub_${branch} /bin/bash -c "sh /home/rgerkin/fix_permissions; jupyterhub -f /etc/jupyterhub/jupyterhub_config.py"
