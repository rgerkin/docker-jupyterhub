# Branch name
branch=$(git rev-parse --abbrev-ref HEAD)
if [ $branch == "dev" ]; then
  port=9000
else
  port=8000
fi
echo $branch
echo $port

docker run -p ${port}:8000 -d --name jupyterhub_${branch} --volumes-from jupyterhub_master jupyterhub_${branch} /bin/bash -c "jupyterhub -f jupyterhub_config.py"
